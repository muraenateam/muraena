        "header": [
          "X-Forwarded-For"
        ]
      },
      "response": {
        "header": [
          "Content-Security-Policy",
          "Content-Security-Policy-Report-Only",
          "Strict-Transport-Security",
          "X-XSS-Protection",
          "X-Content-Type-Options",
          "X-Frame-Options",
          "Referrer-Policy",
          "X-Forwarded-For"
        ]
      }
    },
    "drop": [
      {
        "url": "/logout",
        "redirectTo": "https://google.com"
      }
    ],
    "log": {
      "enabled": true,
      "filePath": "muraena_logs.txt",
